{
  "productName": "simplest-file-renamer",
  "appId": "com.simplestfilerenamer.simplestfilerenamer",
  "copyright": "Copyright Â© 2019 Boris Yakubchik",
  "directories": {
    "output": "release/"
  },
  "files": [
    "**/*",
    "!**/*.ts",
    "!*.code-workspace",
    "!LICENSE.md",
    "!package.json",
    "!package-lock.json",
    "!src/",
    "!e2e/",
    "!hooks/",
    "!angular.json",
    "!_config.yml",
    "!karma.conf.js",
    "!tsconfig.json",
    "!tslint.json"
  ],
  "win": {
    "icon": "dist/favicon",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "src/assets/icons.icns",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist",
    "target": [
      "AppImage"
    ]
  }
}
